{
	"name": "dates-le",
	"publisher": "nolindnaidoo",
	"displayName": "Dates-LE",
	"description": "Zero Hassle Date/Time Extraction and Analysis from Logs, Configs, and Code",
	"version": "1.8.0",
	"license": "MIT",
	"author": {
		"name": "OffensiveEdge",
		"url": "https://github.com/OffensiveEdge"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/OffensiveEdge/dates-le.git"
	},
	"bugs": {
		"url": "https://github.com/OffensiveEdge/dates-le/issues"
	},
	"homepage": "https://github.com/OffensiveEdge/dates-le#readme",
	"engines": {
		"vscode": "^1.70.0",
		"node": ">=20.0.0"
	},
	"categories": [
		"Other",
		"Programming Languages",
		"Linters"
	],
	"keywords": [
		"dates",
		"date",
		"time",
		"datetime",
		"extract",
		"extractor",
		"extraction",
		"logs",
		"timestamps",
		"parsing",
		"analysis",
		"timezone",
		"format",
		"conversion",
		"validation",
		"patterns",
		"ranges",
		"anomalies",
		"xml",
		"maven",
		"gradle"
	],
	"icon": "src/assets/images/icon.png",
	"main": "./dist/extension.js",
	"l10n": "./package.nls.json",
	"activationEvents": [
		"onLanguage:json",
		"onLanguage:yaml",
		"onLanguage:yml",
		"onLanguage:csv",
		"onLanguage:xml",
		"onLanguage:javascript",
		"onLanguage:typescript",
		"onLanguage:html",
		"onLanguage:log",
		"onLanguage:plaintext",
		"onCommand:dates-le.extractDates",
		"onCommand:dates-le.postProcess.dedupe",
		"onCommand:dates-le.postProcess.sort",
		"onCommand:dates-le.analyze",
		"onCommand:dates-le.convert",
		"onCommand:dates-le.filter",
		"onCommand:dates-le.validate",
		"onCommand:dates-le.openSettings",
		"onCommand:dates-le.help"
	],
	"capabilities": {
		"virtualWorkspaces": {
			"supported": "limited",
			"description": "%manifest.capability.untrusted-workspaces.desc%"
		},
		"untrustedWorkspaces": {
			"supported": "limited",
			"description": "%manifest.capability.untrusted-workspaces.desc%"
		}
	},
	"contributes": {
		"commands": [
			{
				"command": "dates-le.extractDates",
				"title": "%manifest.command.extract.title%",
				"category": "%manifest.command.category%"
			},
			{
				"command": "dates-le.postProcess.dedupe",
				"title": "%manifest.command.dedupe.title%",
				"category": "%manifest.command.category%"
			},
			{
				"command": "dates-le.postProcess.sort",
				"title": "%manifest.command.sort.title%",
				"category": "%manifest.command.category%"
			},
			{
				"command": "dates-le.openSettings",
				"title": "%manifest.command.settings.title%",
				"category": "%manifest.command.category%"
			},
			{
				"command": "dates-le.analyze",
				"title": "%manifest.command.analyze.title%",
				"category": "%manifest.command.category%"
			},
			{
				"command": "dates-le.convert",
				"title": "%manifest.command.convert.title%",
				"category": "%manifest.command.category%"
			},
			{
				"command": "dates-le.filter",
				"title": "%manifest.command.filter.title%",
				"category": "%manifest.command.category%"
			},
			{
				"command": "dates-le.validate",
				"title": "%manifest.command.validate.title%",
				"category": "%manifest.command.category%"
			},
			{
				"command": "dates-le.help",
				"title": "%manifest.command.help.title%",
				"category": "%manifest.command.category%"
			},
			{
				"command": "dates-le.csv.toggleStreaming",
				"title": "%manifest.command.csv.toggle-streaming.title%",
				"category": "%manifest.command.category%"
			}
		],
		"keybindings": [
			{
				"command": "dates-le.extractDates",
				"key": "ctrl+alt+d",
				"mac": "cmd+alt+d",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "dates-le.extractDates",
					"when": "resourceExtname == .json || resourceExtname == .yaml || resourceExtname == .yml || resourceExtname == .csv || resourceExtname == .xml || resourceExtname == .js || resourceExtname == .ts || resourceExtname == .jsx || resourceExtname == .tsx || resourceExtname == .html || resourceExtname == .log || resourceExtname == .txt",
					"group": "1_modification@1"
				}
			],
			"commandPalette": [
				{
					"command": "dates-le.extractDates"
				},
				{
					"command": "dates-le.postProcess.dedupe"
				},
				{
					"command": "dates-le.postProcess.sort"
				},
				{
					"command": "dates-le.analyze"
				},
				{
					"command": "dates-le.convert"
				},
				{
					"command": "dates-le.filter"
				},
				{
					"command": "dates-le.validate"
				},
				{
					"command": "dates-le.openSettings"
				},
				{
					"command": "dates-le.help"
				},
				{
					"command": "dates-le.csv.toggleStreaming"
				}
			]
		},
		"configuration": {
			"title": "%manifest.settings.title%",
			"properties": {
				"dates-le.copyToClipboardEnabled": {
					"type": "boolean",
					"default": false,
					"description": "%manifest.settings.copy.clipboard.desc%"
				},
				"dates-le.dedupeEnabled": {
					"type": "boolean",
					"default": false,
					"description": "%manifest.settings.dedupe.desc%"
				},
				"dates-le.notificationsLevel": {
					"type": "string",
					"default": "silent",
					"enum": [
						"all",
						"important",
						"silent"
					],
					"enumDescriptions": [
						"%manifest.settings.notifications.level.option.all%",
						"%manifest.settings.notifications.level.option.important%",
						"%manifest.settings.notifications.level.option.silent%"
					],
					"description": "%manifest.settings.notifications.level.desc%"
				},
				"dates-le.openResultsSideBySide": {
					"type": "boolean",
					"default": true,
					"description": "%manifest.settings.open.side-by-side.desc%"
				},
				"dates-le.safety.enabled": {
					"type": "boolean",
					"default": true,
					"description": "%manifest.settings.safety.enabled.desc%"
				},
				"dates-le.safety.fileSizeWarnBytes": {
					"type": "number",
					"default": 1000000,
					"minimum": 1000,
					"description": "%manifest.settings.safety.warn.file-size.desc%"
				},
				"dates-le.safety.largeOutputLinesThreshold": {
					"type": "number",
					"default": 50000,
					"minimum": 100,
					"description": "%manifest.settings.safety.warn.large-output.desc%"
				},
				"dates-le.safety.manyDocumentsThreshold": {
					"type": "number",
					"default": 8,
					"minimum": 1,
					"description": "%manifest.settings.safety.warn.many-documents.desc%"
				},
				"dates-le.showParseErrors": {
					"type": "boolean",
					"default": false,
					"description": "%manifest.settings.parse-errors.desc%"
				},
				"dates-le.statusBar.enabled": {
					"type": "boolean",
					"default": true,
					"description": "%manifest.settings.statusbar.enabled.desc%"
				},
				"dates-le.telemetryEnabled": {
					"type": "boolean",
					"default": false,
					"description": "%manifest.settings.telemetry.desc%"
				},
				"dates-le.csv.streamingEnabled": {
					"type": "boolean",
					"default": false,
					"description": "%manifest.settings.csv.streaming.desc%"
				},
				"dates-le.postProcess.openInNewFile": {
					"type": "boolean",
					"default": true,
					"description": "%manifest.settings.open.new-file.desc%"
				},
				"dates-le.analysis.enabled": {
					"type": "boolean",
					"default": true,
					"description": "%manifest.settings.analysis.enabled.desc%"
				},
				"dates-le.analysis.includeStats": {
					"type": "boolean",
					"default": true,
					"description": "%manifest.settings.analysis.stats.desc%"
				},
				"dates-le.performance.enabled": {
					"type": "boolean",
					"default": true,
					"description": "%manifest.settings.performance.enabled.desc%"
				},
				"dates-le.performance.maxDuration": {
					"type": "number",
					"default": 5000,
					"minimum": 1000,
					"description": "%manifest.settings.performance.max-duration.desc%"
				},
				"dates-le.performance.maxMemoryUsage": {
					"type": "number",
					"default": 104857600,
					"minimum": 1048576,
					"description": "%manifest.settings.performance.max-memory.desc%"
				},
				"dates-le.performance.maxCpuUsage": {
					"type": "number",
					"default": 1000000,
					"minimum": 100000,
					"description": "%manifest.settings.performance.max-cpu.desc%"
				},
				"dates-le.performance.minThroughput": {
					"type": "number",
					"default": 1000,
					"minimum": 100,
					"description": "%manifest.settings.performance.min-throughput.desc%"
				},
				"dates-le.performance.maxCacheSize": {
					"type": "number",
					"default": 1000,
					"minimum": 100,
					"description": "%manifest.settings.performance.max-cache.desc%"
				},
				"dates-le.keyboard.shortcuts.enabled": {
					"type": "boolean",
					"default": true,
					"description": "%manifest.settings.keyboard.shortcuts.enabled.desc%"
				},
				"dates-le.keyboard.extractShortcut": {
					"type": "string",
					"default": "ctrl+alt+d",
					"description": "%manifest.settings.keyboard.extract.shortcut.desc%"
				},
				"dates-le.keyboard.dedupeShortcut": {
					"type": "string",
					"default": "ctrl+alt+e",
					"description": "%manifest.settings.keyboard.dedupe.shortcut.desc%"
				},
				"dates-le.keyboard.sortShortcut": {
					"type": "string",
					"default": "ctrl+alt+s",
					"description": "%manifest.settings.keyboard.sort.shortcut.desc%"
				},
				"dates-le.presets.enabled": {
					"type": "boolean",
					"default": true,
					"description": "%manifest.settings.presets.enabled.desc%"
				},
				"dates-le.presets.defaultPreset": {
					"type": "string",
					"default": "balanced",
					"enum": [
						"minimal",
						"balanced",
						"comprehensive",
						"performance",
						"dates"
					],
					"enumDescriptions": [
						"%manifest.settings.presets.default.option.minimal%",
						"%manifest.settings.presets.default.option.balanced%",
						"%manifest.settings.presets.default.option.comprehensive%",
						"%manifest.settings.presets.default.option.performance%",
						"%manifest.settings.presets.default.option.dates%"
					],
					"description": "%manifest.settings.presets.default.desc%"
				}
			}
		}
	},
	"scripts": {
		"build": "tsc -p ./",
		"clean": "rm -rf dist coverage",
		"clean:i18n": "node -e \"const fs=require('fs');fs.readdirSync('.').filter(f=>f.startsWith('package.nls.')&&f.endsWith('.json')&&f!=='package.nls.json').forEach(f=>fs.unlinkSync(f));try{fs.rmSync('vsix',{recursive:true})}catch(e){}\"",
		"copy:i18n": "cp src/i18n/package.nls.*.json . 2>/dev/null || true",
		"watch": "tsc -watch -p ./",
		"test": "vitest run --pool=threads",
		"test:watch": "vitest --pool=threads",
		"test:coverage": "vitest run --coverage --pool=threads",
		"test:ui": "vitest --ui",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"vscode:prepublish": "npm run build && npm run copy:i18n",
		"package": "npm run copy:i18n && mkdir -p release && npx vsce package --out release/${npm_package_name}-${npm_package_version}.vsix && npm run clean:i18n",
		"package:ls": "vsce ls release/${npm_package_name}-${npm_package_version}.vsix --tree | cat",
		"publish": "vsce publish"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.5",
		"@types/node": "^24.7.1",
		"@types/vscode": "^1.70.0",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"@vscode/test-electron": "^2.5.2",
		"@vscode/vsce": "^3.6.2",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"vscode-nls": "^5.2.0"
	}
}
